{
  "site": {
    "title": "海生的小窝",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/th.jpeg",
    "logo_darkmode": "/images/th.jpeg",
    "logo_width": "128",
    "logo_height": "60",
    "logo_text": "海生的小窝"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 10,
    "summary_length": 200,
    "blog_folder": "blog"
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "Powered by Astro and astroplate.Designed And Developed by [wanghaisheng](https://github/wanghaisheng)"
  },

  "navigation_button": {
    "enable": true,
    "label": "Get Started",
    "link": "https://github.com/wanghaisheng"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "wanghaisheng",
    "meta_image": "/images/og-image.png",
    "meta_description": "notes of life journey by wanghaisheng"
  }
}
